{
  "name": "ag-ui-react-chat",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@ag-ui/client": "^0.0.40",
    "concurrently": "^9.2.1",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "http-proxy-middleware": "^2.0.6",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "@vitejs/plugin-react": "^4.0.0",
    "typescript": "^5.0.0",
    "vite": "^4.0.0"
  },
  "scripts": {
    "dev": "concurrently \"npm run dev:ui\" \"npm run dev:proxy\" \"npm run dev:agent\" --names ui,proxy,agent --prefix-colors blue,green,yellow --kill-others",
    "dev:ui": "vite",
    "dev:agent": "./scripts/run-agent.sh || scripts/run-agent.bat",
    "dev:proxy": "node proxy-server.js",
    "build": "vite build",
    "preview": "vite preview",
    "install:agent": "./scripts/setup-agent.sh || scripts\\setup-agent.bat",
    "postinstall": "npm run install:agent"
  }
}
